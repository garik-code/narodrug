msgid "[{site_name}] A new listing (ID: {listing_id}) has been submitted"
msgstr ""
"[{site_name}] Una nuova pubblicazione (ID: {listing_id}) è stata inserita "
"nel sito."

msgid ""
"Hi {recipient_name},\n"
"                \n"
"A new listing \"{listing_title}\" has been submitted on {listing_date}.\n"
"\n"
"Submitted by {listing_author_name} ({listing_author_email}):\n"
"\n"
"------------------------------------\n"
"{listing_summary}\n"
"------------------------------------\n"
"\n"
"You can view the listing at {listing_url}.\n"
"\n"
"Regards,\n"
"{site_name}\n"
"{site_url}"
msgstr ""
"Ciaoi {recipient_name},\n"
"                \n"
"Una nuova pubblicazione \"{listing_title}\" è stata inserita il "
"{listing_date}.\n"
"\n"
"Inviata da {listing_author_name} ({listing_author_email}):\n"
"\n"
"------------------------------------\n"
"{listing_summary}\n"
"------------------------------------\n"
"\n"
"Puoi visionarla al seguente indirizzo {listing_url}.\n"
"\n"
"Saluti,\n"
"{site_name}\n"
"{site_url}"

